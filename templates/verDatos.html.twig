{% extends "base.html.twig" %}
{% block container %}
    <div class="container">
        <div style="margin-top:55px" class="mainbox col-md-6 col-md-offset-3 col-sm-9 col-sm-offset-2">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <div class="panel-title">Datos del usuario</div>
                </div>

                <div class="panel-body" >
                  <!--INICIO FORMULARIO-->
                    <div class="col-md-9">
                        <dl class="dl-horizontal">
                            <dt> Nombre: </dt>
                            <dd> {{ usuario.nombre }} </dd>
                            
                            <br>
                            <dt> Apellido: </dt>
                            <dd> {{ usuario.apellido }} </dd>

                            <br>
                            <dt> Email: </dt>
                            <dd> {{ usuario.email }} </dd>

                            <br>
                            <dt> Telefono: </dt>
                            <dd> {{ usuario.telefono }} </dd>

                            <br>
                            <dt> Localidad: </dt>
                            <dd> {{ usuario.localidad }} </dd>
                        </dl>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}